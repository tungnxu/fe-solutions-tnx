-color: #041133; font-weight: normal; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale;\n} small { font-size: 80%;\n} button,\n[type='button'],\n[type='reset'],\n[type='submit'] { -webkit-appearance: button;\n} /* Các element HTML5 mặc định kiểu block\n*/\narticle, aside, details, figcaption, figure, footer, header, hgroup, main, nav, section { display: block;\n} /* Header size quy chuẩn với 1rem = 16px, (nếu cần)\nH1: 2rem = 32px\nH2: 1.5rem = 24px\nH3: 1.125rem = 18px H4: 1rem = 16px */\nh1 { font-size: 2rem; }\nh2 { font-size: 1.5rem; }\nh3 { font-size: 1.125rem; }\nh4 { font-size: 1.00rem; }\nh5 { font-size: 0.83rem; }\nh6 { font-size: 0.67rem; } /* Nếu bạn sử dụng Hr\n*/\nhr { border-style: solid; border-width: 1px 0 0; color: inherit; height: 0; overflow: visible;\n}","breadcrumbs":"Standard Base CSS » Kinh nghiệm","id":"4","title":"Kinh nghiệm"},"5":{"body":"css-remedy","breadcrumbs":"Standard Base CSS » Reference","id":"5","title":"Reference"},"6":{"body":"","breadcrumbs":"Header » Header","id":"6","title":"Header"}},"length":7,"save":true},"fields":["title","body","breadcrumbs"],"index":{"body":{"root":{"0":{".":{"6":{"7":{"df":0,"docs":{},"r":{"df":0,"docs":{},"e":{"df":0,"docs":{},"m":{"df":1,"docs":{"4":{"tf":1.0}}}}}},"df":0,"docs":{}},"8":{"3":{"df":0,"docs":{},"r":{"df":0,"docs":{},"e":{"df":0,"docs":{},"m":{"df":1,"docs":{"4":{"tf":1.0}}}}}},"df":0,"docs":{}},"df":0,"docs":{}},"4":{"1":{"1":{"3":{"3":{"df":1,"docs":{"4":{"tf":1.0}}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":1,"docs":{"4":{"tf":2.0}}},"1":{".":{"0":{"0":{"df":0,"docs":{},"r":{"df":0,"docs":{},"e":{"df":0,"docs":{},"m":{"df":1,"docs":{"4":{"tf":1.0}}}}}},"df":0,"docs":{}},"1":{"2":{"5":{"df":0,"docs":{},"r":{"df":0,"docs":{},"e":{"df":0,"docs":{},"m":{"df":1,"docs":{"4":{"tf":1.4142135623730951}}}}}},"df":0,"docs":{}},"5":{"df":1,"docs":{"4":{"tf":1.0}}},"df":0,"docs":{}},"5":{"df":0,"docs":{},"r":{"df":0,"docs":{},"e":{"df":0,"